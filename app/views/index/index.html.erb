var signIn = document.getElementById('div-sign-in');
var register = document.getElementById('div-register');

signIn.onclick = function(){
  document.getElementById('li-sign-in').setAttribute('class', "active");
  document.getElementById('li-register').removeAttribute('class');
  document.getElementById('tab-register').removeAttribute('class');
  document.getElementById('tab-register').setAttribute("class", "tab-pane fade");
  document.getElementById("tab-sign-in").setAttribute("class","tab-pane fade active in");
};

register.onclick = function(){
  document.getElementById('li-register').setAttribute('class', "active");
  document.getElementById('li-sign-in').removeAttribute('class');
  document.getElementById('tab-sign-in').removeAttribute('class');
  document.getElementById('tab-sign-in').setAttribute("class", "tab-pane fade");
  document.getElementById("tab-register").setAttribute("class","tab-pane fade active in");
};
</script>
